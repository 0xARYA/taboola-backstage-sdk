{
  "name": "taboola-backstage-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Taboola Backstage API - Campaign management, reporting, and audience targeting",
  "author": "Arya Alikhani",
  "license": "MIT",
  "homepage": "https://github.com/0xARYA/taboola-backstage-sdk#readme",
  "bugs": {
    "url": "https://github.com/0xARYA/taboola-backstage-sdk/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist coverage",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "typecheck:tests": "tsc --noEmit -p tsconfig.test.json",
    "check": "npm run lint && npm run typecheck && npm run test:run",
    "prepublishOnly": "npm run check && npm run build"
  },
  "keywords": [
    "taboola",
    "backstage",
    "backstage-api",
    "api",
    "sdk",
    "advertising",
    "campaigns",
    "native-ads",
    "native-advertising",
    "content-discovery",
    "programmatic",
    "marketing",
    "ads",
    "reporting",
    "audience",
    "targeting",
    "typescript",
    "nodejs"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ky": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xARYA/taboola-backstage-sdk.git"
  }
}
